{
  "extends": [
    "next/core-web-vitals",
    "airbnb-base"
  ],
  "rules": {
    "object-curly-newline": "off",
    "lines-between-class-members": "off",
    "import/extensions": "off",
    "no-use-before-define": "off",
    "no-shadow": "off",
    "no-underscore-dangle": "off",
    "no-undef": "off",
    "vars-on-top": "warn",
    "prefer-promise-reject-errors": "off",
    "max-len": [
      "error",
      {
        "code": 110
      }
    ]
  },
  "ignorePatterns": [
    ".env*.local"
  ]
}